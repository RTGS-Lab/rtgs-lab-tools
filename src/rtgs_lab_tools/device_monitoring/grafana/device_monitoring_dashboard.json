          "rawQuery": true,
          "rawSql": "WITH device_data AS (\n  SELECT \n    node_id,\n    (elems1->'Kestrel'->'PORT_V'->>0)::float as battery_voltage,\n    (elems1->'Kestrel'->'AVG_P'->>1)::float as system_power,\n    publish_time,\n    ROW_NUMBER() OVER (PARTITION BY node_id ORDER BY publish_time DESC) as rn\n  FROM raw r,\n  jsonb_array_elements((r.message::jsonb)->'Diagnostic'->'Devices') elems1\n  WHERE r.publish_time > NOW() - INTERVAL '1 day'\n    AND r.event = 'diagnostic/v2'\n    AND elems1 ? 'Kestrel'\n    AND is_valid_json(r.message) \n    AND is_valid_time(r.message)\n),\nlatest_per_device AS (\n  SELECT \n    node_id,\n    battery_voltage,\n    system_power,\n    publish_time,\n    CASE WHEN battery_voltage < 3.6 THEN 1 ELSE 0 END as battery_alert,\n    CASE WHEN system_power > 0.364 THEN 1 ELSE 0 END as power_alert,\n    CASE WHEN publish_time < NOW() - INTERVAL '2 hours' THEN 1 ELSE 0 END as offline_alert,\n    CASE \n      WHEN battery_voltage < 3.6 OR system_power > 0.364 OR publish_time < NOW() - INTERVAL '2 hours'\n      THEN 1 ELSE 0\n    END as flagged\n  FROM device_data \n  WHERE rn = 1\n)\nSELECT 'Total Devices' as metric, COUNT(*) as value FROM latest_per_device\nUNION ALL\nSELECT 'Flagged' as metric, SUM(flagged) as value FROM latest_per_device\nUNION ALL\nSELECT 'Battery Alerts' as metric, SUM(battery_alert) as value FROM latest_per_device\nUNION ALL\nSELECT 'Power Alerts' as metric, SUM(power_alert) as value FROM latest_per_device\nUNION ALL\nSELECT 'Offline Devices' as metric, SUM(offline_alert) as value FROM latest_per_device;",
          "refId": "A",
