          "rawQuery": true,
          "rawSql": "WITH device_data AS (\n  SELECT \n    node_id,\n    (elems1->'Kestrel'->'PORT_V'->>0)::float as battery_voltage,\n    (elems1->'Kestrel'->'AVG_P'->>1)::float as system_power,\n    publish_time,\n    ROW_NUMBER() OVER (PARTITION BY node_id ORDER BY publish_time DESC) as rn\n  FROM raw r,\n  jsonb_array_elements((r.message::jsonb)->'Diagnostic'->'Devices') elems1\n  WHERE r.publish_time > NOW() - INTERVAL '1 day'\n    AND r.event = 'diagnostic/v2'\n    AND elems1 ? 'Kestrel'\n    AND is_valid_json(r.message) \n    AND is_valid_time(r.message)\n),\nlatest_per_device AS (\n  SELECT \n    node_id,\n    battery_voltage,\n    system_power,\n    publish_time\n  FROM device_data \n  WHERE rn = 1\n)\nSELECT \n  node_id as \"Device ID\",\n  node_id as \"Device Name\",\n  CASE \n    WHEN battery_voltage < 3.6 AND system_power > 0.364\n    THEN 'Low Battery + High Power'\n    WHEN battery_voltage < 3.6 \n    THEN 'Low Battery'\n    WHEN system_power > 0.364 \n    THEN 'High Power'\n    WHEN publish_time < NOW() - INTERVAL '2 hours'\n    THEN 'Offline'\n    ELSE 'Alert'\n  END as \"Status\",\n  ROUND(battery_voltage::numeric, 2) as \"Battery (V)\",\n  ROUND(system_power::numeric, 3) as \"Power (W)\",\n  ROUND(EXTRACT(EPOCH FROM (NOW() - publish_time))/60::numeric, 1) as \"Minutes Since Seen\",\n  publish_time as \"Last Seen\"\nFROM latest_per_device\nWHERE battery_voltage < 3.6 OR system_power > 0.364 OR publish_time < NOW() - INTERVAL '2 hours'\nORDER BY \n  CASE \n    WHEN battery_voltage < 3.6 THEN 1\n    WHEN system_power > 0.364 THEN 2  \n    WHEN publish_time < NOW() - INTERVAL '2 hours' THEN 3\n    ELSE 4\n  END,\n  publish_time DESC;",
